{
  "apps": [{
    "name": "van-bake",
    "script": "app.js",
    "instances": 1,
    "max_memory_restart": "256M",
    "autorestart": true,
    "env": {
      "NODE_ENV": "production"
    },
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "error_file": "log/err.log",
    "out_file": "log/logs.log"
  }]
}
